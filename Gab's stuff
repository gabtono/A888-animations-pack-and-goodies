Main4Section:NewButton("subject's jump/dash", "it's BLANK", function()
game.StarterGui:SetCore("SendNotification", {
    Title = 'executed';
    Text = "hold shift and press space bar to perform subject's jump/dash";
    Icon = "rbxassetid://2541869220";
    Duration = 5;
    })
    local player = game.Players.LocalPlayer
local dashPower = 85
local dashduration = 0 -- this part useless af, why did i do this
local debounce = false
local function onAction()
    if debounce then
        return
    end
    debounce = true
    local character = player.Character
    if not character then
        debounce = false
        return
    end
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if not humanoid then
        debounce = false
        return
    end
    local currentStates = humanoid:GetState()
    local shouldClearStates = currentStates ~= Enum.HumanoidStateType.None and currentStates ~= Enum.HumanoidStateType.RunningNoPhysics
    if shouldClearStates then
        humanoid:ChangeState(Enum.HumanoidStateType.RunningNoPhysics)
        wait()
    end
    local mouse = player:GetMouse()
    local direction = (mouse.Hit.Position - character.HumanoidRootPart.Position).unit
    local velocity = direction * dashPower
    humanoid.PlatformStand = true
    humanoid.AutoRotate = false
    humanoid.RootPart.Velocity = velocity
    wait(dashDuration)
    humanoid.PlatformStand = false
    humanoid.AutoRotate = true
    if shouldClearStates then
        humanoid:ChangeState(currentStates)
    end
    debounce = false
end
local dash = Enum.KeyCode.Space
local shift = Enum.KeyCode.LeftShift
local shifted = false
game:GetService("UserInputService").InputBegan:Connect(function(input)
    if input.KeyCode == shift then
        shifted = true
    end
    if input.KeyCode == dash and shifted then
        onAction()
                local args = {
    [1] = {
        ["location"] = game:GetService("SoundService"),
        ["directory"] = {
            [1] = "movement",
        },
        ["soundfile"] = "anomaly_jump"
    }
}

workspace.mainGame.remotes.audio_relay:FireServer(unpack(args))
local sound = game:GetService("ReplicatedStorage").sound_library.movement.anomaly_jump
sound:clone().Parent = game:GetService("SoundService")
sound:Play()
    end
end)
game:GetService("UserInputService").InputEnded:Connect(function(input)
    if input.KeyCode == shift then
        shifted = false
    end
end)
end)
Main4Section:NewButton("knight's dash (execute in lobby)", "it's BLANK", function()
game.StarterGui:SetCore("SendNotification", {
    Title = 'executed';
    Text = "press space bar to perform knight's dash";
    Icon = "rbxassetid://2541869220";
    Duration = 5;
    })
--// Services
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local StarterGui = game:GetService("StarterGui")
local Players = game:GetService("Players")
local DashCD = false

--// Variables
local localPlayer = Players.LocalPlayer
local mouse = localPlayer:GetMouse()
local DashCrap = UserInputService.InputEnded:Connect(function(input, gameproccessedevent)
  if input.KeyCode == Enum.KeyCode.Space then
      if gameproccessedevent then
         return
         end
        if DashCD == true then
            return
        end
DashCD = true

        local animation = localPlayer.Character.Humanoid.Animator:LoadAnimation(game:GetService("ReplicatedStorage").animation_sets.ai_animations.knight.dash)
        local direction = localPlayer.Character.PrimaryPart.CFrame.Position + Vector3.new(0, .5, 0)
        local bodyPosition = Instance.new("BodyPosition")
        bodyPosition.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        bodyPosition.P = 27500

        if UserInputService:IsKeyDown(Enum.KeyCode.W) == true then direction -= localPlayer.Character.PrimaryPart.CFrame.ZVector * 18 end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) == true then direction -= localPlayer.Character.PrimaryPart.CFrame.XVector * 18 end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) == true then direction += localPlayer.Character.PrimaryPart.CFrame.ZVector * 18 end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) == true then direction += localPlayer.Character.PrimaryPart.CFrame.XVector * 18 end

        if UserInputService:IsKeyDown(Enum.KeyCode.W) == false and UserInputService:IsKeyDown(Enum.KeyCode.A) == false and UserInputService:IsKeyDown(Enum.KeyCode.S) == false and UserInputService:IsKeyDown(Enum.KeyCode.D) == false then direction += localPlayer.Character.PrimaryPart.CFrame.ZVector * 18 end

        local args = {
    [1] = {
        ["location"] = game:GetService("SoundService"),
        ["directory"] = {
            [1] = "abilities",
        },
        ["soundfile"] = "dash"
    }
}

workspace.mainGame.remotes.audio_relay:FireServer(unpack(args))

local ye = game:GetService("ReplicatedStorage").sound_library.abilities.dash
ye:clone().Parent = game:GetService("SoundService")
ye:Play()
        bodyPosition.Position = direction
        bodyPosition.Parent = localPlayer.Character.PrimaryPart
        animation:Play()

        task.wait(.2)
        bodyPosition:Destroy()

        task.wait(.2)
        DashCD = false
  end
end)

localPlayer.Character.Humanoid.Died:Connect(function()
DashCrap:Disconnect()
end)
end)
