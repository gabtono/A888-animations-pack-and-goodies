game.StarterGui:SetCore("SendNotification", {
    Title = 'executed';
    Text = "press o or i to zoom out or zoom in";
    Icon = "rbxassetid://2541869220";
    Duration = 5;
    })
game.Workspace.Camera.CameraType = "Custom"
game.Players.LocalPlayer.CameraMode = "Classic"
game.Players.LocalPlayer.CameraMaxZoomDistance = 150
end)
HomemadeSection:NewButton("infinite jump", "it's BLANK", function()
game.StarterGui:SetCore("SendNotification", {
    Title = 'executed';
    Text = "now you can jump while in midair";
    Icon = "rbxassetid://2541869220";
    Duration = 5;
    })
local player = game.Players.LocalPlayer
local isJumping = false
local function handleJump()
    if not isJumping and player.Character and player.Character:FindFirstChild("Humanoid") then
        local humanoid = player.Character.Humanoid
        isJumping = true
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        wait(0.1)
        isJumping = false
    end
end
game:GetService("UserInputService").JumpRequest:Connect(handleJump)
end)
