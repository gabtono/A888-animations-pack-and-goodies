soundsets["voices"].line2.SoundId = soundinto2["demon"].damage2.SoundId
soundsets["voices"].line3.SoundId = soundinto2["demon"].damage3.SoundId
soundsets["voices"].line4.SoundId = soundinto2["demon"].damage4.SoundId
soundsets["voices"].line5.SoundId = soundinto2["demon"].damage1.SoundId
