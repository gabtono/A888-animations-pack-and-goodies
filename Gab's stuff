print([[


---== keybinds ==---
noght demon:
R - demon slam
T - demon slashes
Y - demon rage
U - demon charger
H - demon defeated
G - demon roar
J - demon stomp
M - demon crouch
[ - alternate demon attack
' - demon hurt
grev mothar:
C - grave attack
V - grave defeated
L - grave axe throw
] - grave requiem
B - grave crouch
; - grave hurt
deadbodybloom (corpseflower):
X - flower/bloom roar
Z - flower/bloom defeated
Q - flower/bloom lunge
, - flower/bloom hurt
E - flower/bloom attack
terror stuff:
K - appears
N - disappears


]])
UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if gameProcessedEvent == true then
        return
    end

	if input.KeyCode == Enum.KeyCode.Comma then
	local sounds = {
   {
        directory = {"terrors", "bloom"},
        soundfile = "damage1"
    },
    {
        directory = {"terrors", "bloom"},
        soundfile = "damage2"
    },
      {
        directory = {"terrors", "bloom"},
        soundfile = "damage3"
    }
}

local randomSound = sounds[math.random(1, #sounds)]

local args = {
    {
        location = game:GetService("SoundService"),
        directory = randomSound.directory,
        soundfile = randomSound.soundfile
    }
}

workspace.mainGame.remotes.audio_relay:FireServer(unpack(args))

local soundToPlay = game:GetService("ReplicatedStorage").sound_library[randomSound.directory[1]][randomSound.directory[2]][randomSound.soundfile]
soundToPlay:Clone().Parent = game:GetService("SoundService")
soundToPlay:Play()
	local playerAnimation = LocalPlayer.Character.Humanoid.Animator:LoadAnimation(game.ReplicatedStorage.animation_sets["ai_animations"].meleestagger)
    playerAnimation.Priority = Enum.AnimationPriority.Action
    playerAnimation:Play()
	end
end)
