local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
function Script()
local Window = Library.CreateLib("purito karamell", "GrapeTheme")
--// Main
local Main = Window:NewTab("Main")
local Homemade = Window:NewTab("Homemade")
local HomemadeSection = Homemade:NewSection("something i've made for better gameplay")
local MainSection = Main:NewSection("for acolyte")
local Main3Section = Main:NewSection("for mercenery (buy htf blade for it to work)")
local Main2Section = Main:NewSection("terrors, anomalies movement (execute 1)")
local Main4Section = Main:NewSection("subject and anomaly abilities (execute 1)")
--// The stuff
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService = game:GetService("UserInputService")
local LocalPlayer = game.Players.LocalPlayer
local soundinto = ReplicatedStorage.sound_library.abilities
local soundinto2 = ReplicatedStorage.sound_library.terrors
